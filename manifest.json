{
  "manifest_version": 2,

  "name": "Quickly",
  "description": "This extension allows individuals to clear page history at the click of a button (or hot-key) and (or) close the browser",
  "version": "1.0",
  "browser_action": {
    "default_icon": "128.png",
    "default_popup": "popup.html"
  },
  "icons": { "16": "16.png",
             "19": "19.png",
             "38": "38.png",
             "48": "48.png",
            "128": "128.png" },
  "permissions": [
    "activeTab",
    "browsingData",
    "tabs",
    "history",
    "https://ajax.googleapis.com/"
  ]
}